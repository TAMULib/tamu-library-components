{
  "name": "tamu-library-components",
  "version": "0.0.5",
  "scripts": {
    "build": "ng build --prod --output-hashing none",
    "build:clean": "npm run clean && npm run build",
    "build:tl-components": "node scripts/build-tl-components.js",
    "clean": "rimraf dist",
    "lint": "ng lint",
    "ng": "ng",
    "postbuild": "npm run build:tl-components",
    "start": "ng serve --port 3000",
    "start:static": "node scripts/serve-static.js",
    "start:docker": "docker run  -p 80:80 --name weaver-components-1  weaver-components",
    "test": "npm run test:unit && npm run test:e2e",
    "test:audit": "rimraf .lighthouseci && lhci autorun --upload.target=temporary-public-storage --config=./lighthouserc.json && node scripts/build-tl-components-lighthouse-badges.js",
    "test:e2e": "ng e2e",
    "test:unit": "ng test --no-watch",
    "test:watch": "ng test",
    "test:coverage": "ng test --no-watch --code-coverage",
    "test:report": "cat ./coverage/tamu-library-components/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:ci": "npm run build && npm run test:coverage",
    "build:docs": "npm run build:docs-setup && npm run build:docs-development",
    "build:docs-setup": "node scripts/build-tl-components-docs.js",
    "build:docs-development": "compodoc --output ./static/tamu-library-components/docs/development -p ./src/tsconfig.docs.json",
    "build:docs-production": "npm run build:docs && ls -A | grep -v docs | xargs rimraf && mv docs/* . && rimraf docs",
    "build:static": "npm run build:static-docs && npm run build:static-reports",
    "build:static-setup": "node scripts/build-tl-components-static.js",
    "build:static-docs": "npm run test:audit && npm run build:static-setup && npm run build:docs-development",
    "build:static-reports": "npm run build:static-setup && ng test --no-watch --code-coverage",
    "build:static-production": "npm run build:static && ls -A | grep -v static | xargs rimraf && mv static/tamu-library-components/* . && rimraf static",
    "static:serve": "static-server static -p 8081",
    "static:clean": "rimraf static"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.1",
    "@angular/common": "^9.0.1",
    "@angular/compiler": "^9.0.1",
    "@angular/core": "^9.0.1",
    "@angular/elements": "^9.0.1",
    "@angular/forms": "^9.0.1",
    "@angular/localize": "^9.1.6",
    "@angular/platform-browser": "^9.0.1",
    "@angular/platform-browser-dynamic": "^9.0.1",
    "@angular/router": "^9.0.1",
    "@ng-bootstrap/ng-bootstrap": "^5.3.0",
    "@wvr/elements": "0.0.5",
    "angular-tslint-rules": "^1.20.3",
    "bootstrap": "^4.4.1",
    "classlist.js": "^1.1.20150312",
    "document-register-element": "^1.14.3",
    "elements-zone-strategy": "^9.0.0",
    "ie11-custom-properties": "^3.0.6",
    "lighthouse": "^6.0.0",
    "lighthouse-badges": "^1.0.27",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "^9.0.2",
    "@angular/compiler-cli": "^9.0.1",
    "@angular/language-service": "^9.0.1",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.5.4",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.7.1",
    "codelyzer": "^5.2.1",
    "concat": "^1.0.3",
    "coveralls": "^3.0.9",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "protractor": "^5.4.3",
    "rimraf": "^3.0.2",
    "static-server": "^2.2.1",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "typescript": "~3.7.5"
  }
}
