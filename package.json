{
  "name": "tamu-library-components",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build": "ng build --prod --output-hashing none",
    "build:clean": "npm run clean && npm run build",
    "build:tl-components": "node scripts/build-tl-components.js",
    "clean": "rimraf dist",
    "lint": "ng lint",
    "ng": "ng",
    "postbuild": "npm run build:tl-components",
    "start": "ng serve --port 3000",
    "start:dist": "node scripts/serve-dist.js",
    "start:docker": "docker run  -p 80:80 --name weaver-components-1  weaver-components",
    "test": "npm run test:unit && npm run test:e2e",
    "test:audit": "rimraf .lighthouseci && lhci autorun --upload.target=temporary-public-storage --config=./lighthouserc.json && node scripts/build-tl-components-lighthouse-badges.js",
    "test:e2e": "ng e2e",
    "test:unit": "ng test --no-watch",
    "test:watch": "ng test",
    "test:coverage": "ng test --no-watch --code-coverage",
    "test:report": "cat ./static/tamu-library-components/reports/coverage/tamu-library-components/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:ci": "npm run test:coverage && npm run test:audit",
    "build:docs-setup": "node scripts/build-tl-components-docs.js",
    "build:docs-development": "compodoc --output ./static/tamu-library-components/docs/development -p ./src/tsconfig.docs.json",
    "build:static": "npm run build:static-docs && npm run build:static-reports",
    "build:static-setup": "node scripts/build-tl-components-static.js",
    "build:static-docs": "npm run test:audit && npm run build:static-setup && npm run build:docs-setup && npm run build:docs-development",
    "build:static-reports": "npm run build:static-setup && npm run test:coverage",
    "build:static-production": "npm run build:static && ls -A | grep -v static | xargs rimraf && mv static/tamu-library-components/* . && rimraf static",
    "start:static": "static-server static -p 8081",
    "clean:static": "rimraf static"
  },
  "dependencies": {
    "@angular/animations": "^10.0.7",
    "@angular/common": "^10.0.7",
    "@angular/compiler": "^10.0.7",
    "@angular/core": "^10.0.7",
    "@angular/elements": "^10.0.7",
    "@angular/forms": "^10.0.7",
    "@angular/localize": "^10.0.7",
    "@angular/platform-browser": "^10.0.7",
    "@angular/platform-browser-dynamic": "^10.0.7",
    "@angular/router": "^10.0.7",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@wvr/elements": "1.2.0",
    "bootstrap": "^4.5.0",
    "classlist.js": "^1.1.20150312",
    "document-register-element": "^1.14.3",
    "ie11-custom-properties": "^4.1.0",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.5",
    "@angular-devkit/schematics": "^10.0.5",
    "@angular/cli": "^10.0.5",
    "@angular/compiler-cli": "^10.0.7",
    "@angular/language-service": "^10.0.7",
    "@compodoc/compodoc": "^1.1.11",
    "@lhci/cli": "^0.4.3",
    "@types/jasmine": "~3.5.12",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.0.27",
    "angular-tslint-rules": "^1.20.4",
    "codelyzer": "^6.0.0",
    "concat": "^1.0.3",
    "coveralls": "^3.1.0",
    "glob": "^7.1.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lighthouse": "^6.1.1",
    "lighthouse-badges": "^1.0.32",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "static-server": "^2.2.1",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.3",
    "typescript": "~3.9.7"
  }
}
