{
  "name": "tamu-library-components",
  "version": "1.7.3",
  "private": true,
  "config": {
    "DOCKER_SERVER": "10.55.134.48/weaver/tl-components"
  },
  "scripts": {
    "build": "ng build --prod --single-bundle --output-hashing none",
    "build:clean": "npm run clean && npm run build",
    "build:docs-development": "compodoc --output ./static/tamu-library-components/docs/development -p ./src/tsconfig.docs.json",
    "build:docs-setup": "node scripts/build-tl-components-docs.js",
    "build:tl-components": "node scripts/build-tl-components.js",
    "build:static": "npm run build:static-docs && npm run build:static-reports",
    "build:static-docs": "npm run test:audit && npm run build:static-setup && npm run build:docs-setup && npm run build:docs-development && npm run build:docs-usage",
    "build:static-production": "npm run build:static && node ./scripts/build-tl-components-configuration.js defaults-ci-overrides.env && ls -A | grep -v static | xargs rimraf && mv static/tamu-library-components/* . && rimraf static",
    "build:static-reports": "npm run build:static-setup && npm run test:coverage",
    "build:static-setup": "node scripts/build-tl-components-static.js",
    "build:docker": "node scripts/build-docker.js",
    "build:docs-usage": "wvr-ud",
    "clean": "npm run clean:dist && npm run clean:static",
    "clean:dist": "rimraf dist",
    "clean:static": "rimraf static",
    "publish:docker": "node scripts/docker-push.js",
    "link": "npm link @wvr/elements",
    "lint": "ng lint",
    "ng": "ng",
    "postbuild": "npm run build:tl-components",
    "start": "node ./scripts/build-tl-components-configuration.js defaults-dev-overrides.env && ng serve --port 3000",
    "start:dist": "node ./scripts/build-tl-components-configuration.js defaults-dist-overrides.env && node ./scripts/serve-dist.js",
    "start:static": "node ./scripts/build-tl-components-configuration.js defaults-static-overrides.env && static-server static -p 8082",
    "start:docker": "node scripts/start-docker.js",
    "test": "npm run test:unit && npm run test:e2e",
    "test:audit": "rimraf .lighthouseci && lhci autorun --upload.target=temporary-public-storage --config=./lighthouserc.json && node scripts/build-tl-components-lighthouse-badges.js",
    "test:coverage": "ng test --no-watch --code-coverage",
    "test:ci": "npm run test:coverage && npm run test:audit",
    "test:e2e": "ng e2e",
    "test:report": "cat ./static/tamu-library-components/reports/coverage/tamu-library-components/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:unit": "ng test --no-watch",
    "test:watch": "ng test"
  },
  "dependencies": {
    "@angular/animations": "^11.0.4",
    "@angular/common": "^11.0.4",
    "@angular/compiler": "^11.0.4",
    "@angular/core": "^11.0.4",
    "@angular/elements": "^11.0.4",
    "@angular/forms": "^11.0.4",
    "@angular/localize": "^11.0.4",
    "@angular/platform-browser": "^11.0.4",
    "@angular/platform-browser-dynamic": "^11.0.4",
    "@angular/router": "^11.0.4",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "@ngrx/effects": "^10.1.1",
    "@ngrx/entity": "^10.1.1",
    "@ngrx/store": "^10.1.1",
    "@ngrx/store-devtools": "^10.1.1",
    "@wvr/elements": "1.8.0",
    "bootstrap": "^4.5.3",
    "document-register-element": "^1.14.5",
    "handlebars": "^4.7.6",
    "jsdom": "^16.4.0",
    "karma-viewport": "^1.0.7",
    "ng-inline-svg": "^11.0.1",
    "ng-lazyload-image": "^9.1.0",
    "ngx-build-plus": "^10.1.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular-devkit/schematics": "^11.0.4",
    "@angular/cli": "^11.0.4",
    "@angular/compiler-cli": "^11.0.4",
    "@angular/language-service": "^11.0.4",
    "@compodoc/compodoc": "^1.1.11",
    "@lhci/cli": "^0.6.1",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.14",
    "angular-tslint-rules": "^1.20.4",
    "codelyzer": "^6.0.1",
    "concat": "^1.0.3",
    "console-stamp": "^0.2.9",
    "coveralls": "^3.1.0",
    "dotenv-override-true": "^6.2.2",
    "glob": "^7.1.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lighthouse": "^6.5.0",
    "lighthouse-badges": "^1.0.37",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "static-server": "^2.2.1",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5"
  }
}
